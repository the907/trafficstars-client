{
    "name": "adcrafters/trafficstarsclient",
    "description": "TrafficStars Api Client",
    "keywords": [
        "AdCrafters",
        "trafficstarsclient"
    ],
    "homepage": "https://github.com/adcrafters/trafficstarsclient",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Brevig",
            "email": "michaelbrevig@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "pestphp/pest": "^1.20",
        "symfony/var-dumper": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "AdCrafters\\TrafficStars\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdCrafters\\TrafficStars\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
